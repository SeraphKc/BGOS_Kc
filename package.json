{
  "name": "bgos-kc-monorepo",
  "productName": "BG OS AI Assistant",
  "version": "1.0.0",
  "description": "AI-assistant monorepo with desktop and mobile apps",
  "main": ".webpack/main",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "dev": "NODE_ENV=development electron-forge start",
    "dev:debug": "NODE_ENV=development electron-forge start --inspect",
    "dev:tools": "NODE_ENV=development electron-forge start --devtools",
    "desktop": "electron-forge start",
    "mobile": "pnpm --filter @bgos/mobile start",
    "build:shared": "pnpm --filter '@bgos/shared-*' build"
  },
  "keywords": [],
  "author": {
    "name": "Denis ZHigulin && Denis Klimov",
    "email": "denis.zhigulin@plus8soft.com || denis.klimov@plus8soft.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "css-loader": "^6.11.0",
    "electron": "36.5.0",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@11labs/react": "^0.1.4",
    "@bgos/shared-state": "workspace:*",
    "@bgos/shared-types": "workspace:*",
    "@floating-ui/react": "^0.27.14",
    "@reduxjs/toolkit": "^2.8.2",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "form-data": "^4.0.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.178.0"
  }
}
